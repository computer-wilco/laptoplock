{
  "name": "laptoplock",
  "version": "1.0.0",
  "description": "Zorgt dat mijn laptop vergrendeld wordt",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "update": "npx -y Jelmerro/nus",
    "build-windows": "electron-builder -w",
    "build-mac": "electron-builder -m dmg",
    "build-mac-zip": "electron-builder -m zip",
    "build-linux": "electron-builder -l",
    "build-all": "electron-builder -w -l -m zip",
    "build-linux-unpacked": "electron-builder -l && rm dist/*"
  },
  "keywords": [
    "electron"
  ],
  "author": "Wilco Joosen",
  "license": "Apache-2.0",
  "dependencies": {
    "socket.io-client": "4.8.1"
  },
  "devDependencies": {
    "electron": "35.1.5",
    "electron-builder": "26.0.12"
  }
}